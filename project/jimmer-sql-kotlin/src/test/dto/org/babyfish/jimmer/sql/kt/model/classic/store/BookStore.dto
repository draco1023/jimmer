input BookStoreNullableInput {
    #allScalars
    books? {
        #allScalars
    }
}

input BookStoreNullableIdInput {
    #allScalars
    id(books)? as bookIds
}

dynamic input DynamicBookStoreInput {
    #allScalars?
}

specification BookStoreSpecification {
    name
    notNull(website) as isWebsiteNotNull
    null(website) as isWebsiteNull
}